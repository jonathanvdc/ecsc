// A compare-test test description.

build
{
    $(compiler) $(Filename).cs $(Flags);
}

run
{
    // We can't use the .NET runtime to run these files,
    // so we're just trying to make sure that they actually
    // compile without errors for now.
    // return = @($(net-runtime) $(Executable) $(Args));
}

configs
{
    Og
    {
        Flags = -Og -platform wasm -o $(Executable) -Wno-build;
    }
    O3
    {
        Flags = -O3 -platform wasm -o $(Executable) -Wno-build;
    }
}

init
{
    compiler = $ecsc;
    Filename = Struct;
    Executable = bin/$(Filename).wast;
    Args = ;
}
